require 'spec_helper'

describe UsersController do

  describe "GET 'name:sting'" do
    it "returns http success" do
      get 'name:sting'
      response.should be_success
    end
  end

  describe "GET 'email:string'" do
    it "returns http success" do
      get 'email:string'
      response.should be_success
    end
  end

end
